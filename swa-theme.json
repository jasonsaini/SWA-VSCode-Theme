{
    "name": "Southwest",
    "type": "dark",
    "colors": {
        "editor.background": "#000000",
        "editor.foreground": "#FFFFFF",
        "editorCursor.foreground": "#FECA09",
        "editor.lineHighlightBackground": "#6B84AF22",
        "editorLineNumber.foreground": "#6B84AF",
        "editor.selectionBackground": "#6B84AF44",
        "editor.inactiveSelectionBackground": "#6B84AF22",
        "editorWhitespace.foreground": "#6B84AF55",
        "activityBar.background": "#DD371C",
        "activityBar.inactiveForeground": "#DD371C99",
        "activityBar.foreground": "#FFFFFF",
        "activityBarBadge.background": "#FECA09",
        "activityBarBadge.foreground": "#000000",
        "sideBar.background": "#111111",
        "sideBar.foreground": "#DD371C",
        "statusBar.background": "#DD371C",
        "statusBarItem.hoverBackground": "#FECA09",
        "titleBar.activeBackground": "#DD371C",
        "titleBar.inactiveBackground": "#DD371C99",
        "button.background": "#FECA09",
        "button.foreground": "#000000",
        "button.hoverBackground": "#DD371C",
        "dropdown.background": "#6B84AF",
        "dropdown.foreground": "#FFFFFF",
        "input.background": "#333333",
        "input.foreground": "#DD371C",
        "input.placeholderForeground": "#6B84AF",
        "scrollBar.shadow": "#000000",
        "scrollBarSlider.background": "#6B84AF55",
        "scrollBarSlider.hoverBackground": "#6B84AF88",
        "scrollBarSlider.activeBackground": "#6B84AF",
        "badge.background": "#FECA09",
        "badge.foreground": "#000000",
        "tab.activeBackground": "#6B84AF",
        "tab.inactiveBackground": "#333333",
        "tab.inactiveForeground": "#6B84AF88",
        "tab.activeForeground": "#FFFFFF",
        "tab.hoverBackground": "#6B84AF",
        "tab.unfocusedHoverBackground": "#6B84AF55",
        "terminal.background": "#000000",
        "terminal.foreground": "#DD371C",
        "terminalCursor.background": "#FFFFFF",
        "terminalCursor.foreground": "#FECA09"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#6B84AF"
            }
        },
        {
            "name": "Keywords",
            "scope": ["keyword"],
            "settings": {
                "foreground": "#DD371C"
            }
        },
        {
            "name": "Strings",
            "scope": ["string"],
            "settings": {
                "foreground": "#FECA09"
            }
        },
        {
            "name": "Functions",
            "scope": ["entity.name.function"],
            "settings": {
                "foreground": "#6B84AF"
            }
        },
        {
            "name": "Variables",
            "scope": ["variable"],
            "settings": {
                "foreground": "#DD371C"
            }
        }
    ]
}
