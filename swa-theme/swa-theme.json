{
  "name": "Southwest Softer",
  "type": "dark",
  "colors": {
      "editor.background": "#1E1E1E",
      "editor.foreground": "#D4D4D4",
      "editorCursor.foreground": "#ffab03",
      "editor.lineHighlightBackground": "#fb151d22",
      "editorLineNumber.foreground": "#858585",
      "editor.selectionBackground": "#ffffff44",
      "activityBar.background": "#1544a8",
      "activityBar.foreground": "#ffffff",
      "activityBarBadge.background": "#fb151d",
      "activityBarBadge.foreground": "#ffffff",
      "sideBar.background": "#252526",
      "sideBar.foreground": "#D4D4D4",
      "statusBar.background": "#1544a8",
      "statusBar.foreground": "#ffffff",
      "titleBar.activeBackground": "#1E1E1E",
      "titleBar.activeForeground": "#D4D4D4",
      "button.background": "#1544a8",
      "button.foreground": "#ffffff",
      "dropdown.background": "#1E1E1E",
      "dropdown.foreground": "#D4D4D4",
      "input.background": "#1E1E1E",
      "input.foreground": "#D4D4D4",
      "input.placeholderForeground": "#858585",
      "scrollBar.shadow": "#000000",
      "scrollBarSlider.background": "#fb151d55",
      "scrollBarSlider.hoverBackground": "#fb151d88",
      "scrollBarSlider.activeBackground": "#fb151d",
      "badge.background": "#fb151d",
      "badge.foreground": "#ffffff",
      "tab.activeBackground": "#1E1E1E",
      "tab.inactiveBackground": "#2D2D2D",
      "tab.inactiveForeground": "#858585",
      "tab.activeForeground": "#D4D4D4",
      "tab.hoverBackground": "#1E1E1E",
      "terminal.background": "#1E1E1E",
      "terminal.foreground": "#D4D4D4"
  },
  "tokenColors": [
      {
          "scope": ["comment", "punctuation.definition.comment"],
          "settings": {
              "foreground": "#6A9955"
          }
      },
      {
          "scope": ["keyword"],
          "settings": {
              "foreground": "#C586C0"
          }
      },
      {
          "scope": ["string"],
          "settings": {
              "foreground": "#CE9178"
          }
      },
      {
          "scope": ["entity.name.function"],
          "settings": {
              "foreground": "#DCDCAA"
          }
      },
      {
          "scope": ["variable"],
          "settings": {
              "foreground": "#9CDCFE"
          }
      }
  ]
}
