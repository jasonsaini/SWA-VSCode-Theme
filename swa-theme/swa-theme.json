    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#FFFFFF55"
            }
        },
        {
            "name": "Keywords",
            "scope": ["keyword"],
            "settings": {
                "foreground": "#fb151d"
            }
        },
        {
            "name": "Strings",
            "scope": ["string"],
            "settings": {
                "foreground": "#ffab03"
            }
        },
        {
            "name": "Functions",
            "scope": ["entity.name.function"],
            "settings": {
                "foreground": "#fb151d"
            }
        },
        {
            "name": "Variables",
            "scope": ["variable"],
            "settings": {
                "foreground": "#fb151d"
            }
        },
        {
            "name": "Punctuation",
            "scope": ["punctuation"],
            "settings": {
                "foreground": "#ffab03"
            }
